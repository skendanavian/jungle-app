<section class="admin-products-index">

  <header class="page-header">
    <h1>Admin &raquo; All Categories</h1>
  </header>

  <div class="well">
    <%= link_to '+ New Category', [:new, :admin, :category], class: 'btn btn-info' %>
  </div>

  <div class="panel panel-default products">

    <table class="table table-bordered">
      <thead>
        <tr>
          <th colspan="2">Product</th>
          <th>Category</th>

        </tr>
      </thead>
      <tbody>
      <% @category.each do |category| %>
  <tr>
    <td><%= category.name %></td>
    
    <%# Currently Stuck on This Line -->  Need to get it to route to edit view %>
    <td><%= link_to 'Edit', [:edit, :admin, :category]%></td>

  </tr>
<% end %>
       
      </tbody>
    </table>

  </div>

</section>